{
  "version": 2,
  "buildCommand": "cd client/graph-project-react-app && npm install && npm run build",
  "outputDirectory": "client/graph-project-react-app/build",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/client/graph-project-react-app/api/index.js" }
  ],
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}