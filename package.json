{
  "name": "graph-project",
  "version": "1.0.0",
  "description": "Graphene - Research Paper Graph Visualization",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "server": "cd server && npm start",
    "client": "cd client/graph-project-react-app && npm start",
    "build": "cd client/graph-project-react-app && npm run build",
    "test": "cd client/graph-project-react-app && npm test"
  },
  "dependencies": {
    "firebase-admin": "^13.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
